{
  "name": "WebpackProjectTemplate",
  "version": "0.0.1",
  "description": "Webpack 模板项目",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test webpack --config build/webpack.prod.conf.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "start": "npm run dev",
    "devtest": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js"
  },
